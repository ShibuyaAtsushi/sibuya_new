        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60"
            texture="texplane" />
        <material name="geom" texture="texgeom" texuniform="true" />
    </asset>

    <worldbody>

        <!-- <geom type="plane" size="50 50 .01" rgba="0.6 1 0 1" /> -->
