<mujoco>
    <compiler angle="degree" coordinate="local" inertiafromgeom="true" />
    <option integrator="RK4" timestep="0.0001" />

    <worldbody>
         <light pos="0 -0.5 4"/>
                 <geom conaffinity="1" condim="3" name="floor" friction="0.0 0.0 0.0" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>
            <body name="disk" pos="0 0 5">
                <!-- <geom name="diii" type="cylinder" size="0.5 0.1" rgba=".5 .5 1 1" zaxis="0 1 0" /> -->
                <geom name="diii" type="box" size="1 1 1"/>
                <joint name='slider_x' type='slide' pos='0 0 0' axis='1 0 0'/>
                <joint name='slider_y' type='slide' pos='0 0 0' axis='0 1 0'/>
                <joint name='slider_z' type='slide' pos='0 0 0' axis='0 0 1'/>
                <joint name="wheel_joint1" type="hinge" axis="0 1 0" limited="false" damping="0" />
                <joint name="wheel_joint2" type="hinge" axis="1 0 0" limited="false" damping="0" />
                <joint name="wheel_joint3" type="hinge" axis="0 0 1" limited="false" damping="0" />
                <site type="box" size = "0.12 0.12 0.4" rgba=".5 1 .5 1"/>
            </body>
            <body name="saka1" pos="0 0 2">
                <geom name="saka" type="box"  size="3 3 0.1" euler="10 0 0" />
                </body>
    </worldbody>

    <contact>
                <pair name="masatu" geom1="diii" geom2="saka" friction="1 1 "/>
            </contact>

